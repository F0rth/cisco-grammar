require 'ip'

grammar Pix_object_group_network
	
	include IP
	
	rule object
		espace? 'network-object' espace? network_range espace? eol?
	end
	
	
	rule ip_address
		IPv4address
	end
	
	
	rule network_range
	 ip_address espace? mask
	end
	
	rule mask
	 ip_address
	end


	rule eol
	 [\x0D]*  [\x0A]* [\x0D]*
	end
	
	rule espace
	 [" "]*
	end
end
